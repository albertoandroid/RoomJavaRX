<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.androiddesdecero.roomrx.MainActivity">

    <Button
        android:text="Professor"
        android:id="@+id/mainActivityBtProfessor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:text="Cursos"
        android:id="@+id/mainActivityBtCursos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:text="Lenguajes"
        android:id="@+id/mainActivityBtLanguajes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:text="InnerJoin Profesor Lenguaje"
        android:id="@+id/mainActivityBtInnerJoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>

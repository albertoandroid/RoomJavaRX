<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.androiddesdecero.roomrx.ui.innerjoinprofesorlenguaje.InnerJoinProfesorLenguajeActivity">

    <EditText
        android:hint="Profesor Id"
        android:id="@+id/innerJoinActivityEtProfesorId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <EditText
        android:hint="Salvar"
        android:id="@+id/innerJoinActivityEtLenguajeId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:hint="Salvar"
        android:id="@+id/innerJoinActivityBtSalvar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:hint="Leer"
        android:id="@+id/innerJoinActivityBtLeerProfesor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:hint="Leer Lenguajes"
        android:id="@+id/innerJoinActivityBtLeerLenguaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
